{
  "name": "angular-el03",
  "version": "0.0.0",
  "main": "dist/electron/main.js",
  "build": {
    "productName": "myApp",
    "appId": "com.electron.myapp",
    "directories": {
      "output": "relese"
    },
    "files": [
      "dist"
    ],
    "win": {
      "icon": "dist/elicon.ico",
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:renderer": "ng build --extraWebpackConfig webpack.extra.js",
    "build:prod:renderer": "ng build --prod --extraWebpackConfig webpack.extra.js",
    "build:dev:main": "webpack --config ./webpack.electron.config.ts --env.development",
    "build:prod:main": "webpack --config ./webpack.electron.config.ts --env.production",
    "build:dev:all": "run-s build:renderer build:dev:main",
    "build:prod:all": "run-s build:prod:renderer build:prod:main",
    "electron:dev": "cross-env NODE_EMV=\"development\" electron .",
    "dev:start": "run-s build:dev:all electron:dev",
    "build:win": "npm run build:prod:all && electron-builder --win --x64",
    "package": "electron-packager ./dist myapp --platform=win32 --arch=all --out=release --overwrite --electron-version=9.2.1 --asar",
    "get": "ts-node ./src/app/database/connectionTetst.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.9",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.9",
    "@angular/compiler": "~10.0.9",
    "@angular/core": "~10.0.9",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.0.9",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "~10.0.9",
    "@angular/platform-browser-dynamic": "~10.0.9",
    "@angular/router": "~10.0.9",
    "ngx-electron": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.5",
    "sqlite3": "^5.0.0",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.25",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.6",
    "@angular-eslint/builder": "0.1.0-beta.1",
    "@angular-eslint/eslint-plugin": "0.1.0-beta.1",
    "@angular-eslint/eslint-plugin-template": "0.1.0-beta.1",
    "@angular-eslint/schematics": "0.1.0-beta.1",
    "@angular-eslint/template-parser": "0.1.0-beta.1",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "~10.0.9",
    "@babel/core": "^7.11.4",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.54",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "3.9.0",
    "@typescript-eslint/parser": "3.9.0",
    "babel-eslint": "^10.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "electron": "^9.2.1",
    "electron-builder": "^22.8.0",
    "electron-rebuild": "^2.0.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-build-plus": "^10.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "protractor": "~7.0.0",
    "ts-loader": "^8.0.3",
    "ts-node": "~8.3.0",
    "typescript": "~3.9.5",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
